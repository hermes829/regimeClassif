require(MASS)#
set.seed('6886')#
Sigma <- matrix(c(10,-14,-14,50),2,2)#
Sigma[1,2]/(sqrt(Sigma[1,1])*sqrt(Sigma[2,2])) # correlation coefficient#
dat=mvrnorm(n=100, c(0,200), Sigma)#
# creating confound#
z=apply(dat,1, function(x) FUN=ifelse(x[1]>-1,rbinom(1,1,0.01), rbinom(1,1,0.99)))#
z[z==1]=4#
z[z==0]=1#
zCol=z; zCol[zCol==4]='darkgreen'; zCol[zCol==1]='darkred'#
#
plot(dat[,1],dat[,2], pch=z, las=1, xlim=c(-10,10), xlab='Polity Score', ylab='Number of Labor Protests', col=zCol)#
abline( lm( dat[,2]~dat[,1] ) , lty=3)#
legend('topright', c('Growth','Recession'), pch=c(4,1), col=zCol)
